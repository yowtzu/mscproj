@TechReport{AJ05A,
  contact =	"Michael Kinateder
		 <Kinateder@informatik.uni-stuttgart.de>",
  language =	"English",
  publisher =	"Amsterdam, The Netherlands: IOS Press",
  year = 	"2005",
  keywords =	"trust, reputation, transitive, transitivity,
		 trust-graph, trust-chains",
  ISBN = 	"ISSN: 1570-1263",
  notes =	"UniTEC
		 Homepage==http://unitec.informatik.uni-stuttgart.de;
		 citation: Simplification and Analysis of Transitive
		 Trust Networks. In: Web Intelligence and Agent Systems:
		 An International Journal",
  series =	"Artikel in Zeitschrift",
  institution =  "Universit{\"a}t Stuttgart, Fakult{\"a}t Informatik,
		 Elektrotechnik und Informationstechnik, Germany",
  author =	"Audun Jøsang and Elizabeth Gray and Michael
		 Kinateder",
  month =	{sep},
  type = 	"Article in Journal",
  abstract =	"When transacting and interacting through open computer
		 networks, traditional methods used in the physical
		 world for establishing trust can no longer be used.
		 Creating virtual network substitutes with which people,
		 organisations and software agents can derive trust in
		 other parties requires computerised analysis of the
		 underlying trust networks. This article describes an
		 approach to trust network analysis using subjective
		 logic (TNA-SL), that consists of the three following
		 elements. Firstly it uses a concise notation with which
		 trust transitivity and parallel combination of trust
		 paths can be expressed. Secondly it defnes a method for
		 simplifying complex trust networks so that they can be
		 expressed in this concise form. Finally it allows trust
		 measures to be expressed as beliefs, so that derived
		 trust can be automatically and securely computed with
		 subjective logic. We compare our approach with trust
		 derivation algorithms that are based on normalisation
		 such as PageRank and EigenTrust. We also provide a
		 numerical example to illustrates how TNA-SL can be
		 applied.",
  title =	"Simplification and Analysis of Transitive Trust
		 Networks",
  booktitle =	"Web Intelligence and Agent Systems: An International
		 Journal",
  bibdate =	"August 4, 2005",
  URL =  	"http://unitec.informatik.uni-stuttgart.de",
}

